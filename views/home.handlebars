    <div class="container-fluid">
    {{#if data}}
    {{#each data}}
    {{#if new}}
        <div class="panel panel-warning">
            <div class="panel-heading">
                <h3>{{this.title}}</h3>
            </div>
            <div class="panel-body">
                <a href="{{this.link}}" target="_blank">{{this.link}}</a>
            </div>

        </div>
    {{/if}}
    {{/each}}
        {{else}}
        <div class="panel panel-default no-saved">
            <div class="panel-body">
